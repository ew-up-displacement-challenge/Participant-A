<?xml version="1.0" encoding="us-ascii"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="application">
  <jobTemplates>
    <jobTemplate id="template">
      <streamingExecutable>/application/node/run</streamingExecutable>
<defaultParameters>        
  <parameter id="par1" title="Parameter 1" abstract="Parameter 1" scope="runtime" maxOccurs="1">680</parameter>
  <parameter id="par2" title="Parameter 2" abstract="Parameter 2" scope="runtime" maxOccurs="1">220</parameter>
</defaultParameters>
<defaultJobconf>
                <property id="ciop.job.max.tasks">1</property>
        </defaultJobconf>
</jobTemplate> 
  </jobTemplates>
    <workflow id="workflow" title="EW and Up component extraction" abstract="Sample service to combine the Asc and Desc measurement to extract the EW and Up component of Displacement.">
    <workflowVersion>1.0</workflowVersion>
    <node id="node">
      <job id="template"/>
      <sources>
        <source id="inputdata" scope="runtime" title="References to ascedning and descending data" abstract="References to ascending and descending data as URLs e.g.: file:///application/data/asc_T408.zip,file:///application/data/desc_T472.zip " refid="string:list">file:///application/data/asc_T408.zip,file:///application/data/desc_T472.zip,file:///application/data/asc_T365.zip,file:///application/data/desc_T200.zip</source>
      </sources>
    </node>
  </workflow>
</application>
